  <proxies/>
  <profiles>
    <profile> 
		<activation>
			<activeByDefault>true</activeByDefault>
		</activation>
		<id>restlet</id> 
		<repositories> 
			<repository> 
				<id>maven-restlet</id> 
				<name>Public online Restlet repository</name> 
				<url>http://maven.restlet.org</url> 
			</repository> 
		</repositories> 
		<pluginRepositories> 
			<pluginRepository> 
				<id>maven-restlet</id> 
				<name>Public online Restlet repository</name> 
				<url>http://maven.restlet.org</url> 
			</pluginRepository> 
		</pluginRepositories> 
	</profile> 
   </profiles>

  <activeProfiles/>
